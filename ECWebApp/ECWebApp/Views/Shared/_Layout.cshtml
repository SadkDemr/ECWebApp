                                <li><a asp-controller="Admin" asp-action="CatalogList">Admin</a></li>
                                <li><a href="/Account/Logout">Logout</a></li>
                            </ul>
                        </li>
 }
                    else
                    {
                        <li><a href="/Account/Login"><i class="fa fa-user-o"></i> Login</a></li>
}





                        </ul>



                    </div><!--/.nav-collapse -->
