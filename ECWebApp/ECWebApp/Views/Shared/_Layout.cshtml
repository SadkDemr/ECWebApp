                                    <li><a href="blog-single.html">Single Article</a></li>

                                </ul>
                            </li>


                            <li class="dropdown">
                                <a asp-controller="Home" asp-action="Contact" class="dropdown-toggle" data-toggle="dropdown" data-hover="dropdown" role="button" aria-haspopup="true" aria-expanded="false">Contact Us</a>
